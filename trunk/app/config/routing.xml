<?xml version="1.0" encoding="UTF-8"?>
<configurations xmlns="http://agavi.org/agavi/1.0/config">
	<configuration>
		<routes>
			<route pattern="^/$" module="%actions.default_module%" action="%actions.default_action%" />
			<route pattern="^/browser/$" module="Fs" action="Index" method="read"/>
			<route pattern="^/browser/list/(path:.*)$" module="Fs" action="List" method="read" />
			<route pattern="^/test/$" module="%actions.default_module%" action="%actions.default_action%" />
		</routes>
	</configuration>
</configurations>
